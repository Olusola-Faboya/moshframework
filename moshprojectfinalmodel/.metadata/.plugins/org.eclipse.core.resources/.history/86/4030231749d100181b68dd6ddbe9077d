		public Passenger(ContinuousSpace<Object> space, Grid<Object> grid) {
			this.space = space;
			this.grid = grid;
			cycleOwnership= false;
			carOwnership =false;			
			socialAgreeability= false;
			socialFrequency=0.3;
			interacteeFound=false;
			inquiryInteracteeFound =false;
			isTraveldays =false;
		//	isSatisfied =false;
		//	isCertain=false;
			dNeedsWeight = new HashMap<>();
			dSocialFactor=new HashMap<>();		
			SelectedFriends= new ArrayList<Passenger>();
			similarityValue=0.0;
		}
		@ScheduledMethod(start =0, priority =0)
		public void initialize(){			
			myPerception = new ModeAttributesPerceptions(this.prefferedMode);			
			assumedMaximumDifference= 0.25;			
			socialSatisfaction = 0.0;
			socialInformation =RandomHelper.nextDoubleFromTo(0.02, 0.3);	
			uncertaintyTolerance =uncertaintyToleranceMeasure();
			aspirationLevel =  ComputeAmbition();			
			dEfficiency=0.0;
			dComfortability=0.0;
			dSafety=0.0;	
			dEfficiencyVariance=0.0;
			dComfortabilityVariance=0.0;
			previousExperience=0.0;
			dSafetyVariance=0.0;
			dNeedsWeight = NeedsWeight();
			map= new HashMap<>();
		
		} 
		public Map<SocialWeight, Double> getdSocialFactor() {
			return dSocialFactor;
		}
		@ScheduledMethod(start =1, interval=1)
		public void step() {			
		if (isTraveldays()){
			System.out.println("Tick:"+ this.tickCount);	
			System.out.println("The preferrred mode is "+getPrefferedMode());
	
			//getdUncertainty();	
		//	System.out.println("the unceratinty is:"+ getdUncertainty());
			
			passengerType.socialSimilarity(map);
			myPerception = new ModeAttributesPerceptions(this.prefferedMode);				
////This update the Existence variables
			modeEfficiency = myPerception.updateModeEfficiency();
			modeComfortability =myPerception.updateModeComfortability();
			modeSafety=myPerception.updateModeSafety();
			myPerception.normalisedWeight();			
//	//		determineExistenceSatisfaction();			
//			System.out.println("**************************************************");			
//			System.out.println(" the efficicnecy is"+ getModeEfficiency()+"===== Also form priorotiy 000");	
//			System.out.println("the comfort is"+ getModeComfortability());
//			System.out.println("the safety is"+ getModeSafety()+"====");
//			System.out.println("the previous experience is"+ getPreviousExperience());
///////******************************************************************//				
////			System.out.println(" The Comfort  Uncertainty"+ getdComfortabilityUncertainty());
			getSocialFactor();
	//		System.out.println("The social weithg is: "+ this.dSocialFactor.get(SocialWeight.CONFORMITY));
			passengerType.comformistList(getComformist());
			passengerType.antiComformistList(getAntiComformist());
			passengerType.superirorityList(getSuperiority());			
			computeSocialSatisfcation();
	//		System.out.println("**************************************************");
			computeExperienceSatisfaction();	
			System.out.println("The dSatisfaction is:"+modeSatisfaction);
//			System.out.println("**************************************************");			
//			System.out.println("+*+*+* The  DEfficicnecy is == the real value"+ getdEfficiency());		
//			System.out.println(" The DComfortis"+ getdComfortability());
//			System.out.println("+*+*+*+_ The DSafety is"+ getdSafety());
//			System.out.println("The dSatisfaction is:"+modeSatisfaction);		
//			System.out.println("**************************************************");
			System.out.println("The Social Uncerinty is "+ passengerType.getSocialUncertainty( getSelectedFriends()));
	//		determineVarianceInSatisfaction();
			
		//	updateModeSatisfaction();					
			myPreviousModeStatisfaction();	
			updatePreviousModeEfficiency();
			updatePreviousModeComfortability();
			updatePreviousModeSafety();
			
			levelOfNeedsSatisfaction= getModeSatisfaction();	
			levelOfEfficiencyNeedsSatisfaction= getdEfficiency();	
			levelOfComfortabilityNeedsSatisfaction= getdComfortability();	
			levelOfSafetyNeedsSatisfaction= getdSafety();	
			
			passengerType.updatePreviousExperience();	
			passengerType.updateExistenceEfficiency();
			passengerType.updateExistenceComfortability();			
			passengerType.updateExistenceSafety();	
			
			passengerType.changeInEfficiencyExperience();	
			passengerType.changeInComfortabilityExperience();	
			passengerType.changeInSafetyExperience();	
		
//			System.out.println(" The change in efficiency Satisafction "+ getChangeInEfficiencyExperience());
//			System.out.println(" The change in Comfort Satisafction "+ getChangeInComfortabilityExperience());
//			System.out.println(" The change in Safety Satisafction "+ getChangeInSafetyExperience());
//			System.out.println("===================the Change in deffficiency is"+ getdEfficiency());
//			System.out.println("===================the Effieicnce Neeed Satisf is"+ getLevelOfEfficiencyNeedsSatisfaction());
//			System.out.println("===================the Change in effficiency experience is"+ (getLevelOfEfficiencyNeedsSatisfaction()- getModeEfficiency()) );
			computeExistenceUncertainty();
		
//			System.out.println(" The Safety Uncertainty"+ getdSafetyUncertainty());			
			myPerception.updateModeEfficiencyCognitve();
			myPerception.updateModeSafetyCognitive();
			myPerception.updateModeComfortabilityCognitive();
		/////
			myPerception.updateModeEfficiencyAffective();
			myPerception.updateModeSafetyAffective();
			myPerception.updateModeComfortabilityAffective();
			/////
			myPerception.updateModeSafetyPhysical();
			myPerception.updateModeComfortabilityPhysical();
///******************************************************************			/////	
	
			 decisionSeekingStrategy();
			passengerType.imitation();// selectBehaviour();	
			ComputeAmbition();
	//	passengerType.findWeakLinksInteractee();			
			move();
			}else if (!(isTraveldays())){
			//	System.out.println("======The travel day status is false======");
			}		
		}
		
		public double ComputeAmbition(){
			double totalAmbitions= ((myPerception.getAttrEfficiencyWeight()+myPerception.getAttrComfortabilityWeight()+myPerception.getAttrSafetyWeight())/3);		
			return totalAmbitions;				
		}
		public Map<NeedWeight, Double> NeedsWeight(){
			dNeedsWeight= new HashMap<NeedWeight, Double>();
		//	RandomHelper.nextDoubleFromTo(0.02, 0.3);
			double social = RandomHelper.nextDoubleFromTo(0.0, 0.1);
			dNeedsWeight.put(NeedWeight.EFFICIENCY, myPerception.getAttrEfficiencyWeight());
			dNeedsWeight.put(NeedWeight.COMFORTABILITY, myPerception.getAttrComfortabilityWeight());
			dNeedsWeight.put(NeedWeight.SAFETY, myPerception.getAttrSafetyWeight());
			dNeedsWeight.put(NeedWeight.SOCIAL, social);	
			return dNeedsWeight;				
		}			 
		public Map<NeedWeight, Double> getNeedWeight() {
			return dNeedsWeight;
		}
		
		public Map<SocialWeight, Double> getSocialFactor() {
			return dSocialFactor=passengerType.getSocialSatisfactionFactors(getSelectedFriends());
		}
		
		@ScheduledMethod(start =0, interval=1,priority =0)
		public void determineExistenceSatisfaction(){				
				modeEfficiency= myPerception.getModeEfficiency();//*dNeedsWeight.get(NeedWeight.EFFICIENCY);
				modeComfortability= myPerception.getModeComfortability();//*dNeedsWeight.get(NeedWeight.COMFORTABILITY);
				modeSafety= myPerception.getModeSafety();//*dNeedsWeight.get(NeedWeight.SAFETY);				
				dEfficiency= modeEfficiency;
				dComfortability=modeComfortability;					
				dSafety= modeSafety;	
		}	
/// this moving avreage not useful
		public void determineVarianceInSatisfaction (){	// this is moving average variance			
			double efficiencySatisfaction= getModeEfficiency();//*dNeedsWeight.get(NeedWeight.EFFICIENCY);
			double comfortabilitySatisfaction= getModeComfortability();//*dNeedsWeight.get(NeedWeight.COMFORTABILITY);
			double safetySatisfaction= getModeSafety();//*dNeedsWeight.get(NeedWeight.SAFETY);		
			getTickCount();//count++;
			if(tickCount==1)
		{
			dEfficiency=efficiencySatisfaction;
			dComfortability=comfortabilitySatisfaction;					
			dSafety= safetySatisfaction;	
			System.out.println("d Effic******* is:"+dEfficiency);
			System.out.println("d Comfort******* is:"+dComfortability);
			System.out.println("d Safety******* is:"+dSafety);
			return;
		}	
			double delta=efficiencySatisfaction-dEfficiency;
			dEfficiency =dEfficiency*0.9+efficiencySatisfaction*0.1;
			dEfficiencyVariance= dEfficiencyVariance*0.9+(delta*(efficiencySatisfaction-dEfficiency))*0.1f;
			dComfortability =dComfortability*0.9+comfortabilitySatisfaction*0.1;
			dComfortabilityVariance= dComfortabilityVariance*0.9+(delta*(comfortabilitySatisfaction-dComfortability))*0.1f;
			dSafety =dSafety*0.9+safetySatisfaction*0.1;
			dSafetyVariance= dSafetyVariance*0.9+(delta*(safetySatisfaction-dSafety))*0.1f;
			System.out.println("************************************:");			
//			System.out.println("d Effic is:"+dEfficiency);
//			System.out.println("d Effic variance is:"+dEfficiencyVariance);
//			System.out.println("d Comfort is:"+dComfortability);
//			System.out.println("d Comfort variance is:"+dComfortabilityVariance);
//			System.out.println("d Safety is:"+dSafety);
//			System.out.println("d Safety variance is:"+dSafetyVariance);			
	}	

		
		private double computeExistenceUncertainty(){
			dEfficiencyUncertainty = (Math.pow(getChangeInEfficiencyExperience(),myPerception.getEfficiencyWeight()));//(getdEfficiencyVariance()*dNeedsWeight.get(NeedWeight.EFFICIENCY));
			dComfortabilityUncertainty = (Math.pow(getChangeInComfortabilityExperience(),myPerception.getComfortabilityWeight()));//(getdComfortabilityVariance()*dNeedsWeight.get(NeedWeight.COMFORTABILITY));
			dSafetyUncertainty = (Math.pow(getChangeInSafetyExperience(),myPerception.getSafetyWeight()));//(getdSafetyVariance()*dNeedsWeight.get(NeedWeight.SAFETY));
			double socialunc =passengerType.getSocialUncertainty(getSelectedFriends());
			double	uncertainty = Double.isNaN(((dEfficiencyUncertainty*dComfortabilityUncertainty*dSafetyUncertainty)+socialunc))?0:
				((dEfficiencyUncertainty*dComfortabilityUncertainty*dSafetyUncertainty)+socialunc);//+passengerType.getSocialUncertainty(getSelectedFriends())/2);
			System.out.println(" The Existence Uncertainty is"+ uncertainty);
			 dUncertainty =uncertainty;
			return uncertainty;
			
		}
		private double computeExperienceSatisfaction(){	
//			dEfficiency =getdEfficiency()*dNeedsWeight.get(NeedWeight.EFFICIENCY);
//			dComfortability=getdComfortability()*dNeedsWeight.get(NeedWeight.COMFORTABILITY);
//			dSafety=getdSafety()*dNeedsWeight.get(NeedWeight.SAFETY);//	
		//	double social = computeSocialSatisfcation()*dNeedsWeight.get(NeedWeight.SOCIAL);
			dEfficiency =(Math.pow(getdEfficiency(),myPerception.getEfficiencyWeight()));//getdEfficiency()*dNeedsWeight.get(NeedWeight.EFFICIENCY);
			dComfortability=(Math.pow(getdComfortability(),myPerception.getComfortabilityWeight()));//getdComfortability()*dNeedsWeight.get(NeedWeight.COMFORTABILITY);
			dSafety=(Math.pow(getdSafety(),myPerception.getSafetyWeight()));//getdSafety()*dNeedsWeight.get(NeedWeight.SAFETY);
			double satisfaction = ((dEfficiency*dComfortability*dSafety)+computeSocialSatisfcation());		
			modeSatisfaction=satisfaction;			
			return satisfaction;
		}	

		
////
		private double computeSocialSatisfcation(){
			double conformity = passengerType.comformistList(getComformist());
			double antiConformity=passengerType.antiComformistList(getAntiComformist());
			double superiority =passengerType.superirorityList(getSuperiority());					
		//	System.out.println("The conformity value is:"+conformity);
		//	System.out.println("The Social Weight is:"+dSocialFactor.get(SocialWeight.CONFORMITY));			
			double socialSatisfaction= (Math.pow(conformity,dSocialFactor.get(SocialWeight.CONFORMITY)))*
					(Math.pow(antiConformity,dSocialFactor.get(SocialWeight.ANTI_CONFORMITY)))*
					(Math.pow(superiority,dSocialFactor.get(SocialWeight.SUPERIORITY)));					
		//	System.out.println("********************The dSocialSatisfaction is:"+socialSatisfaction);
			
			return socialSatisfaction;
		}
		
		private int getTickCount(){
			 tickCount = (int) RunEnvironment.getInstance().getCurrentSchedule().getTickCount();
			return tickCount;
		}			
		public double getModeSatisfaction() {
			return modeSatisfaction;
		}
////****This section collect previous experiences***
		private void myPreviousModeStatisfaction(){	myModeSatisfactionList.add(modeSatisfaction);}				
		public List<Double> getModeSatisfactionList() {return myModeSatisfactionList;}
		private void updatePreviousModeEfficiency(){myModeEfficiencyList.add(dEfficiency);}				
		public List<Double> getModeEffciencyList() {return myModeEfficiencyList;}
		private void updatePreviousModeComfortability(){myModeComfortabilityList.add(dComfortability);}				
		public List<Double> getModeComfortabilityList() {return myModeComfortabilityList;}
		private void updatePreviousModeSafety(){myModeSafetyList.add(dSafety);}				
		public List<Double> getModeSafetyList() {return myModeSafetyList;}

		private void move(){ 
			GridPoint pt = grid.getLocation(this);
			NdPoint mypoint = space.getLocation(this);
			NdPoint otherpoint = new NdPoint(pt.getX(),pt.getY());
			double angle = SpatialMath.calcAngleFor2DMovement(space, mypoint, otherpoint);
			space.moveByVector(this, 5, angle,0);
			mypoint=space.getLocation(this);
			grid.moveTo(this,(int)mypoint.getX(),(int)mypoint.getY());
		}
	//	@ScheduledMethod(start =1, interval=1)
		
		public void selectBehaviour(){
			if ((uncertainty>=uncertaintyTolerance)&&(modeSatisfaction<=aspirationLevel)){
				passengerType.inquiring();
				behaviour = DecisionStrategy.INQUIRE;
			}else if ((uncertainty<uncertaintyTolerance)&&(modeSatisfaction<=aspirationLevel)){				
				passengerType.optimising();
				behaviour = DecisionStrategy.OPTIMISE;
			}else if ((uncertainty>uncertaintyTolerance)&&(modeSatisfaction>=aspirationLevel)){				
				passengerType.imitation();
				behaviour= DecisionStrategy.IMITATE;
			}else if ((uncertainty<uncertaintyTolerance)&&(modeSatisfaction>=aspirationLevel)){				
				passengerType.repetition();	
				behaviour= DecisionStrategy.REPEAT;
			}	
			this.behaviour =behaviour;
			adoptInterracteeMode();
		//	updateModeSatisfaction();				
		}
			void decisionSeekingStrategy(){
				boolean satisfied = (getModeSatisfaction()>=satisfiedThreshold)?true:false;
				boolean uncertain = ((getUncertainty()>=uncertaintyThreshold))?true:false;
			if (uncertain &&!satisfied){
				passengerType.inquiring();
				behaviour = DecisionStrategy.INQUIRE;
			}else if (!uncertain && !satisfied){				
				passengerType.optimising();
				behaviour =DecisionStrategy.OPTIMISE;
			}else if (uncertain && satisfied){				
				passengerType.imitation();
				behaviour=DecisionStrategy.IMITATE;
			}else if (!uncertain && satisfied){				
				passengerType.repetition();	
				behaviour=DecisionStrategy.REPEAT;
			}	
			this.behaviour =behaviour;
			adoptInterracteeMode();
		//	updateModeSatisfaction();				
		}
		
		public boolean isCertain(){
			if ((getUncertainty()<=uncertaintyThreshold)){
				return true;
			}else return false;
		}	
		
		public boolean isSatisfied(){
			if ((getModeSatisfaction()>=satisfiedThreshold)){
				return true;
			}else return false;
		}
