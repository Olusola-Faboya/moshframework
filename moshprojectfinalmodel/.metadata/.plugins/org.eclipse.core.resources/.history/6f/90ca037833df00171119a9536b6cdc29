	public void setPhysicalConsideration(){	
			subAttributes = new ArrayList<ModeAttribute>();	
			subAttributes.add(getAttribute("easeOfgettingToMainMode"));	
			subAttributes.add(getAttribute("easeOfgettingOnOffMainMode"));	
			subAttributes.add(getAttribute("distanceToMainMode"));
			subAttributes.add(getAttribute("roadSignAvailabilty"));	
			subAttributes.add(getAttribute("walkingToDestination"));
			
			setEaseOfGettingToMainMode();
			setEaseOfGettingOnOffMainMode();			
			setDistanceToMainMode();
			setRoadSignAvailability();
			setWalkingToDestination();
			
			
			for (ModeAttribute attr : subAttributes){	
				attr.value = getValueOfAttribute(attr.name);
				setValueToAttribute(attr.name, attr.value);	
				subAttributeList.add(attr);
				}
//		
//			for (ModeAttribute attr1 : subAttributeList){	
//				System.out.print("the name is "+ attr1.name+
//						"and"+ "value is"+ attr1.value);			
//				}
			
			physicalConsideration = evaluateHigherAbstraction(subAttributeList);
			setValueToAttribute("physicalConsideration", physicalConsideration);	
		//return physicalConsideration;
		}

	
	public void setCognitiveConsideration(){
		subAttributes = new ArrayList<ModeAttribute>();	
		subAttributes.add(getAttribute("infoReliability"));	
		subAttributes.add(getAttribute("modeFrequency"));	
		subAttributes.add(getAttribute("easeOfInfoAccess"));
		subAttributes.add(getAttribute("easeToDestinationOnTime"));
		subAttributes.add(getAttribute("roadSignAvailabilty"));	
		subAttributes.add(getAttribute("parkingSpaceConcern"));
		
		setInfoReliability();
		setModeFrequency();
		setEaseOfInfoAccess();
		setEaseToDestinationOnTime();
		setRoadSignAvailability();
		setParkingSpaceConcern();
					
		for (ModeAttribute attr : subAttributes){	
			attr.value = getValueOfAttribute(attr.name);
			setValueToAttribute(attr.name, attr.value);	
			subAttributeList.add(attr);
			}		
		cognitiveConsideration = evaluateHigherAbstraction(subAttributeList);
		setValueToAttribute("cognitiveConsideration", cognitiveConsideration);
		
		//return cognitiveConsideration;
		
	}
	
	
	
	public void setAffectiveConsideration(){
