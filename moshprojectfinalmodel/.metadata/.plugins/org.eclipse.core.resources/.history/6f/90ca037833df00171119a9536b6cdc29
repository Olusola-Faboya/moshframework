public class PersonalVehicle extends Mode {

	double cognitiveConsideration;
	double physicalConsideration;
	double affectiveConsideration;
	
	public PersonalVehicle() {
		addAttribute("infoReliability");
		addAttribute("easeOfInfoAccess");
		addAttribute("easeOfgettingToMainMode");
		addAttribute("easeToDestinationOnTime");
		addAttribute("easeOfgettingOnOffMainMode");
		addAttribute("distanceToMainMode");
		addAttribute("modeFrequency");
		addAttribute("parkingSpaceConcern");
		addAttribute("delays");
		addAttribute("security");
		addAttribute("safety");
		addAttribute("roadSignAvailabilty");
		addAttribute("othersAttitude");
		addAttribute("walkingToDestination");
		addAttribute("protectionFromElement");
		/////		
		addAttribute("physicalConsideration");
		setPhysicalConsideration();
		addAttribute("cognitiveConsideration");
		setCognitiveConsideration();
		addAttribute("affectiveConsideration");
		setAffectiveConsideration();
		//
	}
		
	List<ModeAttribute> subAttributes;
	
	
