	}*/	
	

	public  void setPhysicalConsideration(){
		double r = rand.nextDouble();
		if(r<=0.34){			
			setValueToAttribute("physicalConsideration", RandomHelper.nextDoubleFromTo(0.0,0.5));
		}else if (r<=0.34+0.20){		
			setValueToAttribute("physicalConsideration", RandomHelper.nextDoubleFromTo(0.5,0.51));
		}else if (r<=0.34+0.20+0.46){
			setValueToAttribute("physicalConsideration",RandomHelper.nextDoubleFromTo(0.75,0.93));
		}		
	}

	public  void setCognitiveConsideration(){
		double r = rand.nextDouble();
		if(r<=0.34){			
			setValueToAttribute("cognitiveConsideration", RandomHelper.nextDoubleFromTo(0.0,0.5));
		}else if (r<=0.34+0.20){		
			setValueToAttribute("cognitiveConsideration", RandomHelper.nextDoubleFromTo(0.67,0.68));
		}else if (r<=0.34+0.20+0.46){
			setValueToAttribute("cognitiveConsideration",RandomHelper.nextDoubleFromTo(0.67,0.75));
		}		
	}
	
	public  void setAffectiveConsideration(){
		double r = rand.nextDouble();
		if(r<=0.34){			
			setValueToAttribute("affectiveConsideration", RandomHelper.nextDoubleFromTo(0.30,0.5));
		}else if (r<=0.34+0.20){		
			setValueToAttribute("affectiveConsideration", RandomHelper.nextDoubleFromTo(0.6,0.7));
		}else if (r<=0.34+0.20+0.46){
			setValueToAttribute("affectiveConsideration",RandomHelper.nextDoubleFromTo(0.67, 0.69));
		}		
	}
	
}
